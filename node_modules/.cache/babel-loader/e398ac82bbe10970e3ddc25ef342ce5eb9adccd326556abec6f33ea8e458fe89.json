{"ast":null,"code":"import \"core-js/modules/es.array.fill.js\";\nimport \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport _data from '../../data/db';\nexport default {\n  data: function data() {\n    return {\n      questions: _data.questions,\n      title: 'True OR False Quiz',\n      questionIndex: 0,\n      index: 0,\n      rank: null\n    };\n  },\n  computed: {\n    userResponses: function userResponses() {\n      return Array(this.questions.length).fill(false);\n    },\n    pluralizeAnswer: function pluralizeAnswer() {\n      return this.score === 1 ? \"answer\" : \"answers\";\n    }\n  },\n  mounted: function mounted() {\n    if (this.score < 8 || this.score > 5) {\n      this.rank = 'Sorry. Please try again.';\n    } else if (this.score == 8 || this.score < 11) {\n      this.rank = 'Congratulations! You Passed!';\n    } else {\n      this.rank = 'Please know your family more...';\n    }\n  },\n  methods: {\n    next: function next() {\n      this.questionIndex++;\n    },\n    prev: function prev() {\n      this.questionIndex--;\n    },\n    score: function score() {\n      return this.userResponses.filter(function (val) {\n        return val;\n      }).length;\n    }\n  }\n};","map":{"version":3,"mappings":";;;AAgCA,OAAOA,KAAG,MAAO,eAAc;AAC/B,eAAe;EACbA,IAAI,kBAAG;IACL,OAAO;MACLC,SAAS,EAAED,KAAI,CAACC,SAAS;MACzBC,KAAK,EAAE,oBAAoB;MAC3BC,aAAa,EAAE,CAAC;MAChBC,KAAK,EAAE,CAAC;MACRC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAa,EAAE,yBAAW;MACxB,OAAOC,KAAK,CAAC,IAAI,CAACP,SAAS,CAACQ,MAAM,CAAC,CAACC,IAAI,CAAC,KAAK;IAChD,CAAC;IACDC,eAAe,6BAAG;MAChB,OAAO,IAAI,CAACC,KAAI,KAAM,IAAI,QAAO,GAAI,SAAQ;IAC/C;EACF,CAAC;EACDC,OAAO,qBAAG;IACR,IAAI,IAAI,CAACD,KAAI,GAAI,KAAK,IAAI,CAACA,KAAI,GAAI,CAAC,EAAE;MACpC,IAAI,CAACP,IAAG,GAAI,0BAAyB;IACvC,OAAO,IAAI,IAAI,CAACO,KAAI,IAAK,KAAK,IAAI,CAACA,KAAI,GAAI,EAAE,EAAE;MAC7C,IAAI,CAACP,IAAG,GAAI,8BAA6B;IAC3C,OAAO;MACL,IAAI,CAACA,IAAG,GAAI,iCAAgC;IAC9C;EACF,CAAC;EACDS,OAAO,EAAE;IACPC,IAAI,kBAAG;MACL,IAAI,CAACZ,aAAa,EAAE;IACtB,CAAC;IACDa,IAAI,kBAAG;MACL,IAAI,CAACb,aAAa,EAAE;IACtB,CAAC;IACDS,KAAK,EAAE,iBAAW;MAChB,OAAO,IAAI,CAACL,aAAa,CAACU,MAAM,CAAC,UAASC,GAAG,EAAE;QAC7C,OAAOA,GAAE;MAAE,CAAC,CAAC,CAACT,MAAM;IACxB;EACF;AACF","names":["data","questions","title","questionIndex","index","rank","computed","userResponses","Array","length","fill","pluralizeAnswer","score","mounted","methods","next","prev","filter","val"],"sourceRoot":"","sources":["C:\\Users\\Kriss Lee\\Documents\\GitHub\\quiz-vue\\src\\views\\Quiz.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2 class=\"title\">{{ title }}</h2>\r\n      <div class=\"correctAnswers\">\r\n      You have <strong>{{ score() }} </strong>  correct {{ pluralizeAnswer }}!\r\n      </div>\r\n    <div v-for=\"(question, index) in questions\" :key=\"question.id\">\r\n      <div v-show=\"index === questionIndex\">\r\n        <h5>Question number {{ question.id }} out of 10</h5>\r\n        <h2>{{  question.text  }}</h2>\r\n        <div class=\"radio-button\"> \r\n          <li v-for=\"response in question.responses\" :key=\"response.text\">\r\n              <input required type=\"radio\" id=\"radio1\" :value=\"response.text\" :name=\"index\" v-model=\"userResponses[index]\">\r\n              <label for=\"radio1\">{{ response.text }}</label>\r\n          </li>\r\n          <div class=\"btn\">\r\n            <button v-if=\"questionIndex > 0\" class=\"prevnext next\" @click=\"prev\">Prev</button>\r\n            <button class=\"prevnext prev\" @click=\"next\">Next</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n        <div v-show=\"questionIndex === questions.length\">\r\n            <h2>You completed the Quiz!</h2>\r\n            <button v-on:click=\"finalScore\">View your total score!</button>\r\n            <p>Total score: {{ score() }}/{{ questions.length }}</p>\r\n            <p class=\"rank\">{{ rank }}</p>\r\n        </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport data from '../../data/db'\r\nexport default {\r\n  data() {\r\n    return {\r\n      questions: data.questions,\r\n      title: 'True OR False Quiz',\r\n      questionIndex: 0,\r\n      index: 0,\r\n      rank: null\r\n    };\r\n  },\r\n  computed: {\r\n    userResponses: function() {\r\n      return Array(this.questions.length).fill(false)\r\n    },\r\n    pluralizeAnswer() {\r\n      return this.score === 1 ? \"answer\" : \"answers\"\r\n    }\r\n  },\r\n  mounted() {\r\n    if (this.score < 8 || this.score > 5) {\r\n      this.rank = 'Sorry. Please try again.'\r\n    } else if (this.score == 8 || this.score < 11) {\r\n      this.rank = 'Congratulations! You Passed!'\r\n    } else {\r\n      this.rank = 'Please know your family more...'\r\n    }\r\n  },\r\n  methods: {\r\n    next() {\r\n      this.questionIndex++;\r\n    },\r\n    prev() {\r\n      this.questionIndex--;\r\n    },\r\n    score: function() {\r\n      return this.userResponses.filter(function(val) {\r\n        return val }).length;\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  background: #eaeaebc2;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 40%;\r\n  margin-top: -200px;\r\n  margin-left: -200px;\r\n  width: 750px;\r\n  height: 340px;\r\n  align-content: center;\r\n  box-shadow: 5px 5px 10px 5px rgba(61, 2, 85, 0.351);\r\n  color: rgb(33, 0, 37);\r\n  border-radius: 5px;\r\n}\r\n.correctAnswers {\r\n  margin: 10px;\r\n}\r\n.title {\r\n  background: #342c59;\r\n  text-align: center;\r\n  margin: 0;\r\n  padding: 10px;\r\n  color: rgba(255, 254, 254, 0.834);\r\n}\r\nli {\r\n  display: inline-block;\r\n}\r\n .radio-button input[type=\"radio\"] {\r\n        display: hidden;\r\n        margin: auto 10px;\r\n      }   \r\n      .radio-button label {\r\n        display: inline-block;\r\n        background-color: #d1d1d1;\r\n        padding: 10px 40px;\r\n        margin: 5px 20px;\r\n        font-family: sans-serif, Arial;\r\n        font-size: 16px;\r\n        border: 1px solid #342c59;\r\n        border-radius: 4px;\r\n      }\r\n      .radio-button label:hover {\r\n        background-color: #342c59;\r\n        color: #d1d1d1;\r\n      }\r\n      .radio-button input[type=\"radio\"]:focus{\r\n        border: 2px dashed #342c59;\r\n      }\r\n      .radio-button input[type=\"radio\"]:checked + label{\r\n        background-color: #342c59;\r\n        color: #d1d1d1;\r\n      }   \r\n.prevnext {\r\n  font-size: 15px;\r\n  background: none;\r\n  color: #342c59;\r\n  font-family: Arial, Helvetica, sans-serif;\r\n  border-radius: 5px;\r\n  box-shadow: none;\r\n}\r\n.prevnext:active {\r\n  background-color: #342c59;\r\n  color: white;\r\n  box-shadow: none;\r\n}\r\n.prevnext:hover {\r\n  background-color: #342c59;\r\n  color: white;\r\n}\r\n.next {\r\n  margin-right: 500px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
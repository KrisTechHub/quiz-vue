onse.text }}</label>\r\n          </li>\r\n          <div class=\"btn\">\r\n            <button v-if=\"questionIndex > 0\" class=\"prevnext next\" v-on:click=\"prev\">Prev</button>\r\n            <button class=\"prevnext prev\" v-on:click=\"next\">Next</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport data from '../../data/db'\r\nexport default {\r\n  data() {\r\n    return {\r\n      questions: data.questions,\r\n      title: 'True OR False Quiz',\r\n      questionIndex: 0,\r\n      index: 0,\r\n      \r\n    };\r\n  },\r\n  computed: {\r\n    userResponses() {\r\n      Array(questions.length).fill(false)\r\n  },\r\n  correctAnswers() {\r\n            if (this.questions && this.questions.length > 0) {\r\n                  let streakCounter = 0;\r\n                  this.questions.forEach(function (question) {\r\n                    if (!question.rightAnswer) {\r\n                        return;\r\n                    } else if (question.rightAnswer === true) {\r\n                        streakCounter++;\r\n                    }\r\n                });return streakCounter;\r\n            } else {\r\n                return \"--\";\r\n            }\r\n        },\r\n        pluralizeAnswer() {\r\n            return this.correctAnswers === 1 ? \"answer\" : \"answers\"\r\n        },\r\n},\r\n  mounted() {\r\n    console.log('sadas', this.questions)\r\n    // fetch('http://localhost:3000/questions')\r\n    // .then(res => res.json())\r\n    // .then(data => this.jobs = data)\r\n    // .catch(err => console.log(err.message))\r\n  },\r\n  methods: {\r\n    next() {\r\n      this.questionIndex++;\r\n    },\r\n    prev() {\r\n      this.questionIndex--;\r\n    },\r\n    score() {\r\n      return this.userResponses.filter(function(val) {return val }).length\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  background: #eaeaebc2;\r\n  position: absolute;\r\n  top: 5